<h1>You want to see all stories created HERE!</h1>

{{#each handlebarsCall}}
    <p>title: {{dataValues.title}}</p>
    <ul>
        <li>id: {{dataValues.id}}</li>
        <li>genre: {{dataValues.genre}}</li>
        <li>visible to public: {{dataValues.publiclyVisible}}</li>
        <li>open voting to all users: {{dataValues.openVotingToAllUsers}}</li>
        <li>open writing to all users: {{dataValues.openWritingToAllUsers}}</li>
    </ul>

    <button type="button" id="editStory" thisStoryId="{{dataValues.id}}" class="btn btn-info">Edit {{dataValues.title}}</button>
    
    <p>*****</p>   
{{/each}}

